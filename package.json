{
  "name": "svag-dotenv",
  "version": "0.1.8",
  "homepage": "https://github.com/svagcrew/svag-dotenv",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svagcrew/svag-dotenv.git"
  },
  "bugs": {
    "url": "https://github.com/svagcrew/svag-dotenv/issues"
  },
  "author": {
    "name": "Sergei Dmitriev",
    "url": "https://sergei.svag.group"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "type": "module",
  "files": [
    "./src/**/*",
    "./dist/**/*"
  ],
  "scripts": {
    "prettify": "svag-prettier prettify",
    "dev": "svag-ts dev",
    "watch": "svag-ts watch",
    "types": "svag-ts types",
    "build": "pnpm rimraf dist && pnpm tsc --project tsconfig.json && pnpm tsc-alias --project tsconfig.json && pnpm tsc --project tsconfig.cjs.json && pnpm tsc-alias --project tsconfig.cjs.json",
    "start": "svag-ts start",
    "lint": "svag-lint lint",
    "test": "svag-jest test",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "jest": "30.0.0-alpha.4",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "svag-husky": "^0.1.49",
    "svag-jest": "^0.1.39",
    "svag-lint": "^0.1.72",
    "svag-prettier": "^0.1.63",
    "svag-ts": "^0.1.68",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
